package kr.or.ddit.cart.vo;

import java.io.Serializable;

public class DetailCartVO implements Serializable {
    private static final long serialVersionUID = 1L;

    private CartVO cartId;    // CART_ID (VARCHAR2)
    private CartVO optionId;  // OPTION_ID (VARCHAR2)
    private CartVO prodId;    // PROD_ID (VARCHAR2)
    private int dcartQty;     // DCART_QTY (NUMBER)

    public DetailCartVO() {
    }


    public CartVO getCartId() {
        return cartId;
    }

    public void setCartId(CartVO cartId) {
        this.cartId = cartId;
    }

    public CartVO getOptionId() {
        return optionId;
    }

    public void setOptionId(CartVO optionId) {
        this.optionId = optionId;
    }

    public CartVO getProdId() {
        return prodId;
    }

    public void setProdId(CartVO prodId) {
        this.prodId = prodId;
    }

    public int getDcartQty() {
        return dcartQty;
    }

    public void setDcartQty(int dcartQty) {
        this.dcartQty = dcartQty;
    }

}
